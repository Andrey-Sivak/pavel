{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pavel/home-services-showcase",
  "version": "0.1.0",
  "title": "Services Showcase",
  "category": "widgets",
  "description": "Display professional services with heading, introduction, and service cards.",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "heading": {
      "type": "string",
      "default": ""
    },
    "subheading": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "servicesLabel": {
      "type": "string",
      "default": ""
    },
    "services": {
      "type": "array",
      "default": [],
      "items": {
        "type": "object",
        "default": {
          "title": "",
          "description": ""
        }
      }
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "pm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
