{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pavel/post-list",
  "version": "0.1.0",
  "title": "Post List",
  "category": "widgets",
  "description": "Display a list of posts with customizable options.",
  "attributes": {
    "title": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": true,
    "typography": {
      "fontSize": false,
      "lineHeight": false
    },
    "spacing": {
      "padding": false,
      "margin": false
    },
    "color": {
      "background": false,
      "gradients": false,
      "text": false
    }
  },
  "textdomain": "pm",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}
